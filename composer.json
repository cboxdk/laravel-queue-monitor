{
    "name": "gophpeek/laravel-queue-monitor",
    "description": "This is my package laravel-queue-monitor",
    "keywords": [
        "PHPeek",
        "laravel",
        "laravel-queue-monitor"
    ],
    "homepage": "https://github.com/gophpeek/laravel-queue-monitor",
    "license": "MIT",
    "authors": [
        {
            "name": "Sylvester Damgaard",
            "email": "sn@cbox.dk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3 || ^8.4 || ^8.5",
        "gophpeek/laravel-queue-metrics": "^1.0",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^11.0||^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.8",
        "larastan/larastan": "^3.0",
        "orchestra/testbench": "^10.0.0||^9.0.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "PHPeek\\LaravelQueueMonitor\\": "src/",
            "PHPeek\\LaravelQueueMonitor\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHPeek\\LaravelQueueMonitor\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PHPeek\\LaravelQueueMonitor\\LaravelQueueMonitorServiceProvider"
            ],
            "aliases": {
                "LaravelQueueMonitor": "PHPeek\\LaravelQueueMonitor\\Facades\\LaravelQueueMonitor"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}